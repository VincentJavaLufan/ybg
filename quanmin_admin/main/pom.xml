		<dependency>
			<groupId>com.ybg</groupId>
			<artifactId>edu_city</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!-- 教育模块 end -->
		<!-- 支付模块 -->
		<dependency>
			<groupId>com.egzosn</groupId>
			<artifactId>pay-java-common</artifactId>
			<version>${pay-java.version}</version>
