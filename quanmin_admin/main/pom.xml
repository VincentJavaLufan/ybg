			<version>1.0.0</version>
		</dependency>
		<!-- 代码生成 end -->
		<!--云存储 -->
		<dependency>
			<groupId>com.ybg</groupId>
			<artifactId>ybg_oss</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!--云存储 end -->
		<!-- <dependency> <groupId>com.ybg</groupId> <artifactId>ybg_weixin</artifactId> 
			<version>1.0.0</version> </dependency> -->
