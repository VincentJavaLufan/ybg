		<!--验证码end -->
		<!-- 工作流 -->
		<dependency>
			<groupId>com.ybg</groupId>
			<artifactId>ybg_activiti</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!-- 工作流 end -->
		<!-- 二维码 -->
		<dependency>
			<groupId>com.ybg</groupId>
			<artifactId>ybg_qrcode</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!-- 二维码 end -->
		<!-- https 按需引入 <dependency> <groupId>com.ybg</groupId> <artifactId>ybg_https</artifactId> 
			<version>1.0.0</version> </dependency> -->
		<!--https end -->
		<!--
		<dependency>
			<groupId>com.ybg</groupId>
			<artifactId>ybg_uflo</artifactId>
			<version>1.0.0</version>
		</dependency>
	
		<dependency>
			<groupId>com.ybg</groupId>
			<artifactId>ybg_ureport</artifactId>
			<version>1.0.0</version>
		</dependency>
		
		<dependency>
			<groupId>com.ybg</groupId>
			<artifactId>ybg_urule</artifactId>
			<version>1.0.0</version>
		</dependency>-->
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.6.0</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
					<encoding>${project.build.sourceEncoding}</encoding>
					<compilerArguments>
						<verbose />
						<bootclasspath>${java.home}/lib/rt.jar;${java.home}/lib/jce.jar</bootclasspath>
					</compilerArguments>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-assembly-plugin</artifactId>
				<configuration>
					<descriptor>src/main/resources/distribution/gs-spring-boot-distribution.xml</descriptor>
					<finalName>main-1.0.0.exe</finalName>
				</configuration>
				<executions>
					<execution>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<configuration>
					<archive>
						<manifest>
							<mainClass>App</mainClass>
							<addClasspath>true</addClasspath>
							<classpathPrefix>lib</classpathPrefix>
						</manifest>
						<manifestEntries>
							<Claa-Path>./</Claa-Path>
						</manifestEntries>
					</archive>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>